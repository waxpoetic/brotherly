{
  "addons": [
    {
      "plan": "airbrake:hrk-free-2018",
      "as": [
        "AIRBRAKE",
        "BEARACHE"
      ]
    },
    "librato",
    "newrelic",
    "heroku-postgresql",
    "heroku-redis",
    "sendgrid"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-ruby"
    }
  ],
  "env": {
    "ADMIN_EMAIL": {
      "required": true
    },
    "ADMIN_PASSWORD": {
      "required": true
    },
    "AWS_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_S3_BUCKET_NAME": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "AWS_TRANSCODER_PIPELINE_ID": {
      "required": true
    },
    "DOMAIN_NAME": {
      "required": true
    },
    "EVENTBRITE_ANONYMOUS_TOKEN": {
      "required": true
    },
    "EVENTBRITE_API_KEY": {
      "required": true
    },
    "EVENTBRITE_AUTH_TOKEN": {
      "required": true
    },
    "EVENTBRITE_CLIENT_SECRET": {
      "required": true
    },
    "FACEBOOK_ADMIN_ID": {
      "required": true
    },
    "FACEBOOK_APP_ID": {
      "required": true
    },
    "GOOGLE_CALENDAR_CLIENT_ID": {
      "required": true
    },
    "GOOGLE_CALENDAR_CLIENT_SECRET": {
      "required": true
    },
    "HEROKU_APP_NAME": {
      "required": true
    },
    "HEROKU_PARENT_APP_NAME": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "MAILCHIMP_API_KEY": {
      "required": true
    },
    "MAILCHIMP_LIST_ID": {
      "required": true
    },
    "MAX_THREADS": {
      "required": true
    },
    "NEW_RELIC_LOG": {
      "required": true
    },
    "RACK_ENV": {
      "value": "deployment"
    },
    "RAILS_ENV": {
      "value": "production"
    },
    "SECRET_KEY_BASE": {
      "required": true
    },
    "WEB_CONCURRENCY": {
      "required": true
    }
  },
  "formation": {
    "worker": {
      "quantity": 1
    },
    "web": {
      "quantity": 1
    }
  },
  "name": "brotherly",
  "scripts": {
    "postdeploy": "bin/rails db:setup"
  },
  "stack": "heroku-18"
}
